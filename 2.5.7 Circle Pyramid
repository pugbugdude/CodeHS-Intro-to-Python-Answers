speed(10)

penup()
setposition(-100,-200)

for i in range(3):
    pendown()
    circle(50)
    penup()
    forward(100)

penup()
setposition(-50, -115)

for i in range(2):
    pendown()
    circle(50)
    penup()
    forward(100)
    
penup()
setposition(0, -30)

pendown()
circle(50)
penup()
forward(100)
